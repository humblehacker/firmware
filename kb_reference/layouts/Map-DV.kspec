  Key:5C  Map:q_and_Q            tl:"Q"
  Key:5D  Map:j_and_J            tl:"J"
  Key:5E  Map:k_and_K            tl:"K"
  Key:5F  Map:x_and_X            tl:"X"
  Key:5H  Map:b_and_B            tl:"B"
  Key:5J  Map:w_and_W            tl:"W"
  Key:5K  Map:v_and_V            tl:"V"
  Key:5L  Map:z_and_Z            tl:"Z"
