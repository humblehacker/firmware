  Key:2J Map:6_and_^           tl:"^"    bl:"6"
  Key:2K Map:7_and_&           tl:"&"    bl:"7"
  Key:2L Map:8_and_*           tl:"*"    bl:"8"
  Key:2M Map:9_and_(           tl:"("    bl:"9"
  Key:2N Map:0_and_)           tl:")"    bl:"0"

  /* Row 3 */
  Key:3B Map:q_and_Q           tl:"Q"
  Key:3C Map:w_and_W           tl:"W"
  Key:3D Map:e_and_E           tl:"E"
  Key:3E Map:r_and_R           tl:"R"
  Key:3F Map:t_and_T           tl:"T"
  Key:3J Map:y_and_Y           tl:"Y"
  Key:3K Map:u_and_U           tl:"U"
  Key:3L Map:i_and_I           tl:"I"
  Key:3M Map:o_and_O           tl:"O"
