  Key:4K Map:j_and_J           tl:"J"
  Key:4L Map:k_and_K           tl:"K"
  Key:4M Map:l_and_L           tl:"L"

  /* Row 5 */
  Key:5B Map:z_and_Z           tl:"Z"
  Key:5C Map:x_and_X           tl:"X"
  Key:5D Map:c_and_C           tl:"C"
  Key:5E Map:v_and_V           tl:"V"
